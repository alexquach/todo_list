{
  "expo": {
    "name": "TodoListApp",
    "slug": "TodoListApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_Rounded_with_margin.jpg",
    "scheme": "com.alexquach.TodoListApp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexquach.TodoListApp",
      "scheme": "com.alexquach.TodoListApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo_Rounded_with_margin.png",
        "backgroundColor": "#ffffff"
      },
      "scheme": "com.alexquach.TodoListApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo_Rounded_with_margin.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1351f1a7-2f2b-40a5-9ebf-76a2925c91b2"
      }
    }
  }
}
